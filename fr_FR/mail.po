msgid "COMBI_ORDER_CONFIRMATION_DOWN_PAYMENT_AND_INVOICE_HEADER_SHOWROOM_OR_AFTERPAY"
msgstr "<p style=\"margin: 0 0 10px 0\">Votre commande est en cours de traitement.</p>"

msgid "COMBI_ORDER_CONFIRMATION_DOWN_PAYMENT_AND_INVOICE_HEADER_OTHER_PAYMENT"
msgstr "<p style=\"margin: 0 0 10px 0\">Avant que nous puissions poursuivre le traitement de votre commande, il est important que nous recevions le montant restant à payer de %s €.</p><p style=\"margin: 0 0 10px 0\">Le paiement peut être effectué via l’état de commande.</p>"

msgid "COMBI_ORDER_CONFIRMATION_DOWN_PAYMENT_AND_INVOICE_HEADER_SHIPPING"
msgstr "<p style=\"margin: 0 0 10px 0;\">Lorsque votre commande sera remise au transporteur, nous vous enverrons un nouvel e-mail.</p>"

msgid "COMBI_ORDER_CONFIRMATION_DOWN_PAYMENT_AND_INVOICE_HEADER_PICKUP_POINT"
msgstr "<p style=\"margin: 0 0 10px 0;\">Lorsque votre commande est au point de retrait choisi, nous vous enverrons un nouvel e-mail.</p>"

msgid "COMBI_ORDER_CONFIRMATION_DOWN_PAYMENT_AND_INVOICE_HEADER_SHOWROOM"
msgstr "<p style=\"margin: 0 0 10px 0;\">Lorsque votre commande sera arrivée dans notre showroom, nous vous enverrons un nouvel e-mail.</p>"

